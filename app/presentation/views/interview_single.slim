/ !-- Fonts & ICON CSS-->
script src="https://use.fontawesome.com/releases/v5.15.3/js/all.js" crossorigin="anonymous"

title InterviewSharing


<header class="page-section bg-white text-primary text-center">
  <div class="container d-flex align-items-center flex-column" id="page-top">
    <!-- Masthead Heading-->
    <h1 class="masthead-heading mb-0">INTERVIEW SHARING</h1>
    <!-- Icon Divider-->
    <div class="divider-custom">
      <div class="divider-custom-line"></div>
      <div class="divider-custom-icon"><i class="fas fa-star"></i></div>
      <div class="divider-custom-line"></div>
    </div>
    h1 class="text-center text-uppercase text-primary mb-0"="#{interview.position}"
    h5 class="text-center text-uppercase text-secondary mb-0"="#{interview.author}"
    h5 class="text-center text-uppercase text-secondary mb-0"="#{interview.iss_module}"
    </div>
  </div>  
</header>


<nav class="postnav navbar navbar-expand-lg bg-secondary fixed-bottom" id="mainNav">
  <div class="container">
    a class="navbar-brand js-scroll-trigger" href="#page-top" ="#{internship.title}"
    ul class="navbar-nav ml-auto"
      <li class="nav-item mx-0 mx-lg-1"><a class="nav-link py-3 px-0 px-lg-3 rounded js-scroll-trigger" href="#info">INFO</a>
      </li>
      <li class="nav-item mx-0 mx-lg-1"><a class="nav-link py-3 px-0 px-lg-3 rounded js-scroll-trigger" href="#des">DESCRIPTION</a>
      </li>
      <li class="nav-item mx-0 mx-lg-1"><a class="nav-link py-3 px-0 px-lg-3 rounded js-scroll-trigger" href="#adivce">ADVICE</a>
      </li>
      <li class="nav-item mx-0 mx-lg-1"><a class="nav-link py-3 px-0 px-lg-3 rounded js-scroll-trigger" href="#result">RESULT</a>
      </li>
    </ul>
  </div>
</nav>

<section class="page-section bg-primary " id="info">
  <div class="container">
      <!-- Portfolio Section Heading-->
      <div class="text-center">
          <h2 class="page-section-heading text-white text-uppercase mb-0 d-inline-block"><br>Interview Infomation</h2>
      </div>
      <!-- Icon Divider-->
      <div class="divider-custom divider-light">
          <div class="divider-custom-line"></div>
          <div class="divider-custom-icon"><i class="fas fa-star"></i></div>
          <div class="divider-custom-line"></div>
      </div>
      <!-- Portfolio Grid Items-->
      <div class="row justify-content-center mx-3">
          <!-- Portfolio Items-->
          <div class="interviewinfo col-md-6 col-lg-4 mb-5 text-center">Company
            h2 class="interviewinfo2 text-uppercase mb-0" ="#{interview.company_name}"
          </div>
          <div class="interviewinfo col-md-6 col-lg-4 mb-5 text-center">Position
              h2 class="interviewinfo2 text-uppercase mb-0" ="#{interview.position}"
          </div>
          <div class="interviewinfo col-md-6 col-lg-4 mb-5 text-center">Date
              h2 class="interviewinfo2 text-uppercase mb-0" ="#{interview.time}"
          </div>
          <div class="interviewinfo col-md-6 col-lg-4 mb-5 text-center">Location
              h2 class="interviewinfo2 text-uppercase mb-0" ="#{interview.interview_location}"
          </div>
          <div class="interviewinfo col-md-6 col-lg-4 mb-5 text-center">Recruit Source
              h2 class="interviewinfo2 text-uppercase mb-0" ="#{interview.recruit_source}"
          </div>
          
      </div>
      
  </div>
</section>

<section class="page-section bg-secondary text-white mb-0" id="des">
  <div class="container">
      <!-- About Section Heading-->
      <div class="text-center">
          <h2 class="page-section-heading d-inline-block text-white">INTERVIEW DESCRIPTION</h2>
      </div>
      <!-- Icon Divider-->
      <div class="divider-custom divider-light">
          <div class="divider-custom-line"></div>
          <div class="divider-custom-icon"><i class="fas fa-star"></i></div>
          <div class="divider-custom-line"></div>
      </div>
      h4 class="text-center text-uppercase mb-0 text-white"="#{interview.description}"
  </div>
</section>  

<section class="page-section bg-primary text-white mb-0" id="adivce">
  <div class="container">
      <!-- About Section Heading-->
      <div class="text-center">
          <h2 class="page-section-heading d-inline-block text-white">ADVICE</h2>
      </div>
      <!-- Icon Divider-->
      <div class="divider-custom divider-light">
          <div class="divider-custom-line"></div>
          <div class="divider-custom-icon"><i class="fas fa-star"></i></div>
          <div class="divider-custom-line"></div>
      </div>
      h4 class="text-center text-white mb-0"="#{interview.advice}" 
  </div>
</section>

