<section class="page-section" id="contact">
  <div class="container">
      h2 class="page-section-heading text-center text-uppercase text-secondary mb-0" = "#{@current_account.username}"
      <div class="divider-custom">
          <div class="divider-custom-line"></div>
          <div class="divider-custom-icon"><i class="fas fa-star"></i></div>
          <div class="divider-custom-line"></div>
      </div>
      h2 class="page-section-heading text-center text-uppercase text-secondary mb-0" = "Interview"

      <div class="row">
          <div class="col-lg-8 mx-auto">
              form action="/interviews/#{interview.id}" method="POST" role="form" class="form-horizontal"
                  <div class="sentMessage">
                      <div class="form-group floating-label-form-group controls mb-0 pb-2">
                          <label>Position</label>
                          input class="form-control" name="position" type="text" placeholder="Position" required="required" data-validation-required-message="Please enter the position." value="#{interview.position}"
                      </div>
                  </div>

                  br
                    <div class="control-group">
                      <div class="form-row align-items-center">
                        <div class="col-auto" id="ccc">
                          <p>輸入統一編號來尋找公司</p>
                          <label class="sr-only" for="inlineFormInput">Name</label>
                          <input type="text" class="form-control mb-2" id="comp_no" placeholder="tax ID number" value="">
                          <button type="button" class="btn btn-primary mb-2" onclick='searchComp("comp_no")'>Search Company</button>
                        </div>
                      </div>
                        
                      <div class="form-group floating-label-form-group">
                        <label>Company</label>
                        input class="form-control" name="company_name" id="comp_name" type="tel" placeholder="Company" required="required" data-validation-required-message="Please enter your intern company." value="#{interview.company_name}"
                      </div>
                    </div>

                  /time
                  <div class="date-picker">
                    <div class="form-group floating-label-form-group controls mb-0 pb-2">
                      <label>Interview Date</label>
                      input class="form-control" id="datepicker" name="time" type="text" placeholder="Inerview Date" required="required" data-validation-required-message="Please enter the Date." value="#{interview.time}"
                    </div>
                  </div>
                  
                  <div class="control-group">
                      <div class="form-group floating-label-form-group controls mb-0 pb-2">
