                    <label for="select-box3" class="label select-box3">
                        <span class="label-desc">#{interview.recruit_source}</span> 
                    </label>
                    select id="select-box3" class="select" name="recruit_source"
                      <option value="#{interview.recruit_source}" hidden>Choose here</option>
                      <option value="">Choose here</option>
                      <option value="Linkedin">Linkedin</option>
                      <option value="CakeResume">CakeResume</option>
                      <option value="Google Careers">Google Careers</option>
                      <option value="104">104</option>
                      <option value="1111">1111</option>
                      <option value="Yourator">Yourator</option>
                      <option value="Official website">Official website</option>
                      <option value="Other">Other</option>
                    </select>
                  </div>

                  /description
                  <div class="control-group">
                      <div class="form-group floating-label-form-group controls mb-0 pb-2">
                          <label>Description</label>
                          textarea class="form-control" name="description" rows="5" placeholder="Interview Description" required="required" data-validation-required-message="Please enter job description." = "#{interview.description}"
                      </div>
                  </div>

                  /advice
