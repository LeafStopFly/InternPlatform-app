javascript:
  function searchComp(id) {
    var comp_no = document.getElementById(id).value;

    $.ajax({
      type:"POST",
      url:"/companies",
      dataType:"json",
      data: {company_no: comp_no},
      success: function () {
        console.log('it worked!');
      },
      error: function (result) {
        console.log('it failed :(');
        document.getElementById("comp_name").value = result.responseText;
      }
    });
  }

title PostInterview

<section class="page-section" id="contact">
  <div class="container">
      h2 class="page-section-heading text-center text-uppercase text-secondary mb-0" = "#{@current_account.username}"
      <div class="divider-custom">
          <div class="divider-custom-line"></div>
          <div class="divider-custom-icon"><i class="fas fa-star"></i></div>
          <div class="divider-custom-line"></div>
      </div>
      h2 class="page-section-heading text-center text-uppercase text-secondary mb-0" = "Interview"

      <div class="row mx-1">
          <div class="col-lg-8 mx-auto">
              <form action="/interviews" method="POST" role="form" class="form-horizontal">

                  <div class="sentMessage">
                      <div class="form-group floating-label-form-group controls mb-0 pb-2">
                          <label>Interview Position</label>
                          <input class="form-control" name="position" type="text" placeholder="Interview Position" required="required" data-validation-required-message="Please enter the title." />
                      </div>
                  </div>

                  br
                    <div class="control-group">
                      <div class="container">
                        <div class="container" id="ccc">
                          <p></p>
                          <label class="sr-only" for="inlineFormInput">Search Company</label>
                          <input type="text" class="form-control mb-2" id="comp_no" placeholder="輸入統一編號來尋找公司" value="">
                           <button type="button" class="btn btn-primary mb-0" onclick='searchComp("comp_no")'>Search Company</button>
                        </div>
                      </div>
                      <div class="form-group floating-label-form-group">
                        <label>Company</label>
                        <input class="form-control" name="company_name" id="comp_name" type="tel" placeholder="Company" required="required" data-validation-required-message="Please enter your intern company." />
                      </div>   
                    </div>

                  /time
                  <div class="date-picker">
                    <div class="form-group floating-label-form-group controls mb-0 pb-2">
                      <label>Interview Date</label>
                      <input class="form-control" id="datepicker" name="time" type="text" placeholder="Interview Date" required="required" data-validation-required-message="Please enter the Date." />
                    </div>
                  </div>
                  
                  <div class="control-group">
                      <div class="form-group floating-label-form-group controls mb-0 pb-2">
                          <label>Interview Location</label>
                          <input class="form-control" name="interview_location" type="tel" placeholder="Interview Location" required="required" />
                          /<p class="help-block text-danger"></p>
                      </div>
                  </div>

                  /iss_module
                  <div class="select-box">
                    <label for="select-box1" class="label select-box1"><span class="label-desc">Choose your module</span> </label>
                    <select id="select-box1" class="select" name="iss_module">
                      <option value="" ></option>
                      <option value="Information Systems (IS)">Information Systems (IS)</option>
                      <option value="Service Marketing and Management">Service Marketing and Management</option>
                      <option value="Service Innovation and Design">Service Innovation and Design</option>
                      <option value="Business Analytics">Business Analytics</option>
                    </select>
                  </div>

                  /recruit_source
                  <div class="select-box">
                    <label for="select-box3" class="label select-box3"><span class="label-desc">Recruit Source</span> </label>
                    <select id="select-box3" class="select" name="recruit_source">
                      <option value="">Choose here</option>
                      <option value="Linkedin">Linkedin</option>
                      <option value="CakeResume">CakeResume</option>
                      <option value="Google Careers">Google Careers</option>
                      <option value="104">104</option>
                      <option value="1111">1111</option>
                      <option value="Yourator">Yourator</option>
                      <option value="Official website">Official website</option>
                      <option value="Other">Other</option>
                    </select>
                  </div>

                  /description
                  <div class="control-group">
                      <div class="form-group floating-label-form-group controls mb-0 pb-2">
                          <label>Interview Description</label>
                          <textarea class="form-control" name="description" rows="5" placeholder="Interview Description" required="required" data-validation-required-message="Please enter job description."></textarea>
                          /<p class="help-block text-danger"></p>
                      </div>
                  </div>

                  /advice
                  <div class="control-group">
                      <div class="form-group floating-label-form-group controls mb-0 pb-2">
                          <label>Advice</label>
                          <textarea class="form-control" name="advice" rows="5" placeholder="Advice" required="required" data-validation-required-message="Please enter the reactionary."></textarea>
                          /<p class="help-block text-danger"></p>
                      </div>
                  </div>

                  /Waiting result time
                  <div class="select-box">
                    <label for="select-box4" class="label select-box4"><span class="label-desc">Waiting result time</span> </label>
                    <select id="select-box4" class="select" name="waiting_result_time">
                      <option value="">Choose here</option>
                      <option value="3 days">3 days</option>
                      <option value="3-7 days">3-7 days</option>
                      <option value="More than 7 days">More than 7 days</option>
                      <option value="No reply">No reply</option>
                    </select>
                  </div>

                  /result
                  <div class="select-box">
                    <label for="select-box5" class="label select-box4"><span class="label-desc">Result</span> </label>
                    <select id="select-box5" class="select" name="result">
                      <option value="">Choose here</option>
                      <option value="Offer get">Offer get</option>
                      <option value="Didn't get offer">Didn't get offer</option>
                      <option value="No reply">No reply</option>
                    </select>
                  </div>

                  /level
