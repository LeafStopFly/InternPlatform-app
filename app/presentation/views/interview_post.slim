section class="page-section" id="contact"
  div class="container"
      h2 class="page-section-heading text-center text-uppercase text-secondary mb-0" = "#{@current_account.username}"
      <div class="divider-custom">
          <div class="divider-custom-line"></div>
          <div class="divider-custom-icon"><i class="fas fa-star"></i></div>
          <div class="divider-custom-line"></div>
      </div>

      <div class="row">
          <div class="col-lg-8 mx-auto">
              <form id="contactForm" name="sentMessage" novalidate="novalidate">

                  <div class="sentMessage">
                      <div class="form-group floating-label-form-group controls mb-0 pb-2">
                          <label>Position</label>
                          <input class="form-control" id="name" type="text" placeholder="Position" required="required" data-validation-required-message="Please enter the title." />
                          /<p class="help-block text-danger"></p>
                      </div>
                  </div>

                  <div class="sentMessage">
                      <div class="form-group floating-label-form-group controls mb-0 pb-2">
                          <label>Company</label>
                          <input class="form-control" id="name" type="text" placeholder="Company" required="required" data-validation-required-message="Please enter the title." />
                          /<p class="help-block text-danger"></p>
                      </div>
                  </div>


                  /time
                  div class="date-picker"
                    <div class="form-group floating-label-form-group controls mb-0 pb-2">
                      <label>INTERVIEW DATE</label>
                      <input class="form-control" id="datepicker" type="text" placeholder="Inerview Date" required="required" data-validation-required-message="Please enter the Date." />

                  / <div class="control-group">
                  /     <div class="form-group floating-label-form-group controls mb-0 pb-2">
                  /         <label>Email position</label>
                  /         <input class="form-control" id="email" type="email" placeholder="Email Address" required="required" data-validation-required-message="Please enter your email address." />
                  /         <p class="help-block text-danger"></p>
                  /     </div>
                  / </div>
                  
                  <div class="control-group">
                      <div class="form-group floating-label-form-group controls mb-0 pb-2">
                          <label>Interview location</label>
                          <input class="form-control" id="phone" type="tel" placeholder="Interview location" required="required" />
                          /<p class="help-block text-danger"></p>
                      </div>
                  </div>

                  /iss_module
                  <div class="select-box">
                    <label for="select-box1" class="label select-box1"><span class="label-desc">Choose your module</span> </label>
                    <select id="select-box1" class="select">
                      <option value="Choice 1">Information Systems (IS)</option>
                      <option value="Choice 2">Service Marketing and Management</option>
                      <option value="Choice 3">Service Innovation and Design</option>
                      <option value="Choice 4">Business Analytics</option>
                    </select>
                  </div>

                  /time

                  /recruit_source
                  <div class="select-box">
                    <label for="select-box3" class="label select-box3"><span class="label-desc">Recruit Source</span> </label>
                    <select id="select-box3" class="select">
                      <option value="Choice 1">Information Systems (IS)</option>
                      <option value="Choice 2">Service Marketing and Management</option>
                      <option value="Choice 3">Service Innovation and Design</option>
                      <option value="Choice 4">Business Analytics</option>
                    </select>
                  </div>

                  /description
                  <div class="control-group">
                      <div class="form-group floating-label-form-group controls mb-0 pb-2">
                          <label>Description</label>
                          <textarea class="form-control" id="message" rows="5" placeholder="Interview Description" required="required" data-validation-required-message="Please enter job description."></textarea>
                          /<p class="help-block text-danger"></p>
                      </div>
                  </div>

                  /advice
                  <div class="control-group">
                      <div class="form-group floating-label-form-group controls mb-0 pb-2">
                          <label>Advice</label>
                          <textarea class="form-control" id="message" rows="5" placeholder="Advice" required="required" data-validation-required-message="Please enter the reactionary."></textarea>
                          /<p class="help-block text-danger"></p>
                      </div>
                  </div>

                  /Wating result time
                  <div class="select-box">
                    <label for="select-box4" class="label select-box4"><span class="label-desc">Wating result time</span> </label>
                    <select id="select-box4" class="select">
                      <option value="Choice 1">3 days</option>
                      <option value="Choice 2">3-7 days</option>
                      <option value="Choice 3">More than 7 days</option>
                      <option value="Choice 4">No reply</option>
                    </select>
                  </div>

                  /result
                  <div class="select-box">
                    <label for="select-box5" class="label select-box4"><span class="label-desc">Result</span> </label>
                    <select id="select-box5" class="select">
                      <option value="Choice 1">Offer get</option>
                      <option value="Choice 2">Didn't get offer</option>
                      <option value="Choice 3">No reply</option>
                    </select>
                  </div>

                  /level
                  <div class="page__demo">
                    <div class="page__group">
                      <div class="rating">
                        <input type="radio" name="rating-star" class="rating__control screen-reader" id="rc1">
                        <input type="radio" name="rating-star" class="rating__control screen-reader" id="rc2">
                        <input type="radio" name="rating-star" class="rating__control screen-reader" id="rc3">
                        <input type="radio" name="rating-star" class="rating__control screen-reader" id="rc4">
                        <input type="radio" name="rating-star" class="rating__control screen-reader" id="rc5">
                        <label for="rc1" class="rating__item">
                          <svg class="rating__star">
                            <use xlink:href="#star"></use>
                          </svg>
                          <span class="screen-reader">1</span>
                        </label>
                        <label for="rc2" class="rating__item">
                          <svg class="rating__star">
                            <use xlink:href="#star"></use>
                          </svg>
                          <span class="screen-reader">2</span>
                        </label>
                        <label for="rc3" class="rating__item">
                          <svg class="rating__star">
                            <use xlink:href="#star"></use>
                          </svg>
                          <span class="screen-reader">3</span>
                        </label>
                        <label for="rc4" class="rating__item">
                          <svg class="rating__star">
                            <use xlink:href="#star"></use>
                          </svg>
                          <span class="screen-reader">4</span>
                        </label>
                        <label for="rc5" class="rating__item">
                          <svg class="rating__star">
                            <use xlink:href="#star"></use>
                          </svg>
                          <span class="screen-reader">5</span>
                        </label>	
                      </div>
                      <span class="page__hint">Degree of difficulty</span>
                    </div>
                  </div>

                  /rating star
                  <div class="page__demo">
                    <div class="page__group">
                      <div class="rating2">
                        <input type="radio" name="rating2-star" class="rating2__control screen-reader" id="rc6">
                        <input type="radio" name="rating2-star" class="rating2__control screen-reader" id="rc7">
                        <input type="radio" name="rating2-star" class="rating2__control screen-reader" id="rc8">
                        <input type="radio" name="rating2-star" class="rating2__control screen-reader" id="rc9">
                        <input type="radio" name="rating2-star" class="rating2__control screen-reader" id="rc10">
                        <label for="rc6" class="rating2__item">
                          <svg class="rating2__star">
                            <use xlink:href="#star"></use>
                          </svg>
                          <span class="screen-reader">1</span>
                        </label>
                        <label for="rc7" class="rating2__item">
                          <svg class="rating2__star">
                            <use xlink:href="#star"></use>
                          </svg>
                          <span class="screen-reader">2</span>
                        </label>
                        <label for="rc8" class="rating2__item">
                          <svg class="rating2__star">
                            <use xlink:href="#star"></use>
                          </svg>
                          <span class="screen-reader">3</span>
                        </label>
                        <label for="rc9" class="rating2__item">
                          <svg class="rating2__star">
                            <use xlink:href="#star"></use>
                          </svg>
                          <span class="screen-reader">4</span>
                        </label>
                        <label for="rc10" class="rating2__item">
                          <svg class="rating2__star">
                            <use xlink:href="#star"></use>
                          </svg>
                          <span class="screen-reader">5</span>
                        </label>	
                      </div>
                      <span class="page__hint">Rating</span>
                    </div>
                  </div>
                  <br />
                  
                  <div id="success"></div>
                  button type='submit' id='sendMessageButton' value='post' class="btn btn-primary btn-block" POST
              </form>
          </div>
      </div>
  </div>
  
