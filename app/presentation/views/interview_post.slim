javascript:
  function searchComp(id) {
    var comp_no = document.getElementById(id).value;

    $.ajax({
      type:"POST",
      url:"/companies",
      dataType:"json",
      data: {company_no: comp_no},
      success: function () {
        console.log('it worked!');
      },
      error: function (result) {
        console.log('it failed :(');
        document.getElementById("comp_name").value = result.responseText;
      }
    });
  }

<section class="page-section" id="contact">
