  function DeleteInternship(id, title) {
    console.log("/internships/"+id+"/delete")

    $.ajax({
      type:"POST",
      url:"/internships/"+id+"/delete",
      dataType:"json",
      headers: {
      'Content-Type': 'application/json'
      },
      data: {intern_id: id},
      success:function(result){
          alert(result);
        },
      error:function(result){
        console.log(result);
        window.location = result.responseText;
      }
    });
  }

  function DeleteInterview(id, title) {
    console.log("/interviews/"+id+"/delete")

    $.ajax({
      type:"POST",
      url:"/interviews/"+id+"/delete",
      dataType:"json",
      headers: {
      'Content-Type': 'application/json'
      },
      data: {interv_id: id},
      success:function(result){
          alert(result);
        },
      error:function(result){
        console.log(result);
        window.location = result.location;
      }
    });
  }

